{
  "name": "ssg-template",
  "version": "1.0.0",
  "private": true,
  "description": "ssg-template",
  "author": "pavel",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "preload-fonts": "gatsby-preload-fonts --no-sandbox",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/_sprite.svg --stripAttrs id",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax=scss -q",
    "prettier": "npx prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "0.27.2",
    "gatsby": "4.14.0",
    "gatsby-plugin-image": "2.14.1",
    "gatsby-plugin-manifest": "^4.15.0",
    "gatsby-plugin-preload-fonts": "3.14.0",
    "gatsby-plugin-react-helmet": "5.14.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-sass": "5.14.0",
    "gatsby-plugin-sharp": "4.14.1",
    "gatsby-source-filesystem": "4.14.0",
    "gatsby-transformer-sharp": "4.14.0",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "8.0.1",
    "react-simple-modal-provider": "^1.9.0",
    "redux": "4.2.0",
    "reselect": "4.1.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.4.2",
    "@reduxjs/toolkit": "1.8.1",
    "@types/node": "17.0.31",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.16",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "7.1.24",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "sass": "1.51.0",
    "stylelint": "13.6.1",
    "stylelint-declaration-use-variable": "1.7.3",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.2.0",
    "svg2sprite-cli": "^2.0.1",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.{tsx,ts}": "eslint --fix",
    "*.scss": "stylelint --fix \"src/**/*.scss\" --syntax=scss -q"
  }
}
